    "jingtum"
  ],
  "author": "Lospring Liu <lospringliu@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "git+https://github.com/swtcca/swtclib.git"
  },
  "homepage": "https://github.com/swtcca/swtclib",
  "dependencies": {
    "axios": "^0.18.0",
    "swtc-transaction": "^1.2.16"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@types/utf8": "^2.1.6",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "esm": "^3.2.22",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "swtc-tum3": "^0.1.2",
    "tslint": "^5.16.0",
    "tum3-eth-abi": "^1.0.2",
    "typescript": "^3.4.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "babel": {
    "presets": [],
    "plugins": []
